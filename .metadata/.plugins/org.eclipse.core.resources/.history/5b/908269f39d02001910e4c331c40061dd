package beans;

import java.util.List;
import java.util.Map;
import java.util.Set;

public class Test {
	
	private List<String>fruits;
	private Set<String> cricketers;
	private Map CountryCapitals;
	
	public Test()
	{
		System.out.println("Test object created");
	}
	
	public void setFruit(List<String> f)
	{
		fruits = f;
	}
	
	public void setCriceters(Set<String> c)
	{
		cricketers = c;
	}
	
	public void setCountryCapitals(Map c)
	{
		CountryCapitals = c;
	}
	
	public void printDetails()
	{
		for(Object each: fruits)
		{
			System.out.println("Fruits: "+each);
		}
		
		for(Object each: cricketers)
		{
			System.out.println("Cricketers: "+each);
		}
		
		for(Object  each: CountryCapitals.keySet())
		{
			System.out.println("Country capitals: "+each + "=" + CountryCapitals.get(each));
		}
	}
}
