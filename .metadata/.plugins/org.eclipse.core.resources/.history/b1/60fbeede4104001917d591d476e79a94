package findKthTermSortedArray;

import java.util.Scanner;

public class Driver {
	public static void main(String[] args)
	{
		int[] array1 = {1,2,3,4,5,6,7};
		int[] array2 = {0,8,9,10};
		
		FindKTerm f = new FindKTerm(array1, array2, 2);
		
		System.out.println("The number at the given index is "+f.findTerm(f.getMid1(),0));
	}
}
