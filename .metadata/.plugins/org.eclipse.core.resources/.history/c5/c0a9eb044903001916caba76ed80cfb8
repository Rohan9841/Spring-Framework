package twoDimensionMatrix;

public class Driver {
	
	public static void main(String[] args)
	{
		int[][] m = new int[5][3];
		
		int[] num = {1,0,5,2,3,4,9,0,0,10,8,7,0,11,12};
		
		int counter = 0;
		
		for(int i = 0; i<m[0].length; i++)
		{
			for (int j = 0; j<m[1].length; j++)
			{
				m[i][j] = counter;
				counter++;
			}
		}
		
		System.out.println(m);
	}
}
